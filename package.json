{
  "name": "vue-with-nuxt",
  "version": "1.0.0",
  "description": "Vue.js with Nuxt.js Framework",
  "author": "nurrahmanfajar <nurrahmanfajar93@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt -c config/index.js",
    "build": "nuxt build -c config/index.js --environment=production",
    "start": "nuxt start -c config/index.js --environment=production",
    "generate": "nuxt generate -c config/index.js --environment=production",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .eslintignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "nuxt": "^1.0.0-rc3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6"
  }
}
